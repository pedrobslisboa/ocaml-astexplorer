{
  "name": "ocaml-astexplorer",
  "version": "2.0.0",
  "description": "OCaml AST explorer for Reason and OCaml",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/pedrobslisboa/ocaml-astexplorer.git"
  },
  "author": "Pedro Lisboa",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/plugin-transform-runtime": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-react": "^7.16.0",
    "autoprefixer": "^10.0.0",
    "babel-loader": "^8.0.4",
    "concurrently": "^6.2.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.2",
    "eslint": "^6.6.0",
    "eslint-import-resolver-webpack": "^0.12.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-react": "^7.0.1",
    "eslint-plugin-require-in-package": "^1.0.3",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^3.2.0",
    "inline-manifest-webpack-plugin": "^4.0.1",
    "mini-css-extract-plugin": "^0.9.0",
    "null-loader": "^4.0.0",
    "postcss-loader": "^4.0.0",
    "raw-loader": "^4.0.0",
    "rimraf": "^3.0.2",
    "serve": "^11",
    "style-loader": "^1.1.3",
    "terser-webpack-plugin": "^2.2.1",
    "url-loader": "^4.1.0",
    "webpack": "^4.17.2",
    "webpack-cli": "^3.1.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.16.3",
    "astexplorer-refmt": "^2.0.0",
    "codemirror": "^5.65.16",
    "font-awesome": "^4.5.0",
    "isomorphic-fetch": "^2.2.1",
    "json-stringify-safe": "^5.0.1",
    "lodash.isequal": "^4.5.0",
    "postcss": "^8.0.0",
    "prop-types": "^15.5.10",
    "react": "16",
    "react-dom": "^16.0.1",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  },
  "browserslist": "> 0.25%, not dead",
  "scripts": {
    "dev": "concurrently \"yarn start\" \"yarn build-dev\"",
    "start": "serve -l 8080 ../out",
    "build": "rimraf ../out/* && cross-env NODE_OPTIONS=--openssl-legacy-provider NODE_ENV=production webpack --mode=production",
    "build-dev": "rimraf ../out/* && cross-env NODE_OPTIONS=--openssl-legacy-provider NODE_ENV=development webpack -d --mode=development",
    "watch": "cross-env NODE_OPTIONS=--openssl-legacy-provider webpack -dw --mode=development",
    "lint": "node_modules/eslint/bin/eslint.js src/",
    "fontcustom": "fontcustom compile ./fontcustom/input-svg/ --config=./fontcustom/config.yml"
  }
}
